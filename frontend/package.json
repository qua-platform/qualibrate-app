{
  "name": "entropy-frontend-ui",
  "version": "1.0.0",
  "description": "Web Interface for Entropy",
  "author": "QM & V4Scale",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "homepage": "/",
  "scripts": {
    "build": "PUBLIC_PATH='/static/' webpack",
    "build:inside-docker": "PUBLIC_PATH='/static/' webpack",
    "start": "webpack serve --open",
    "start:local:dev": "env-cmd -f .env webpack serve --open",
    "start:dev": "API_URL=https://dev.entropy-lab.io/ REACT_APP_MQTT_HOST=wss://dev.entropy-lab.io/updates/ws webpack serve --open",
    "clean": "rimraf ./dist",
    "start:testServer": "node scripts/testServer.js",
    "lint": "run-s lint:code lint:style",
    "lint:code": "eslint --ext ts,tsx src",
    "lint:style": "stylelint --fix -f verbose src/**/*.scss",
    "lint:fix": "npm run lint:code -- --fix && npm run lint:style -- --fix",
    "start:mqtt:test": "cross-env API_URL=http://localhost/ cross-env ENABLE_AUTO_AUTH=TRUE cross-env REACT_APP_MQTT_HOST=ws://localhost/updates/ws npm run start",
    "start:local": "cross-env API_URL=http://localhost/ npm run start",
    "cypress": "cypress open",
    "cypress-functional": "npx cypress run --browser chrome --spec cypress/e2e/functional/* --reporter dot",
    "cypress:demo:e2e": "npx cypress run --browser chrome --spec cypress/e2e/demo/* --reporter dot --config-file cypress.demo.config.ts",
    "prepare": "cd .. && husky install entropy_frontend_ui/.husky",
    "format": "prettier --write src cypress ../docs public package.json"
  },
  "pre-commit": [
    "format"
  ],
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/plotly.js": "^2.12.18",
    "@types/react": "18.0.28",
    "@types/react-datepicker": "4.10.0",
    "@types/react-dom": "18.0.11",
    "@types/refractor": "3.0.2",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "assert": "^2.0.0",
    "cypress": "12.8.1",
    "env-cmd": "^10.1.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "express": "4.18.2",
    "husky": "^8.0.3",
    "npm-run-all": "4.1.5",
    "pre-commit": "1.2.2",
    "prettier": "2.8.6",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-strict-scss": "^0.5.0",
    "stylelint-prettier": "3.0.0",
    "typescript": "5.0.2",
    "webpack-dev-server": "4.13.1"
  },
  "dependencies": {
    "@h5web/app": "^7.0.1",
    "@mui/material": "^5.13.5",
    "@react-hook/switch": "1.3.3",
    "@types/cytoscape": "3.19.9",
    "assets": "3.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "css-hot-loader": "1.4.4",
    "css-loader": "^6.7.3",
    "cytoscape": "3.23.0",
    "date-fns": "2.29.3",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "flexlayout-react": "0.6.10",
    "html-webpack-plugin": "^5.5.0",
    "ify-loader": "^1.1.0",
    "javascript-time-ago": "2.5.9",
    "mini-css-extract-plugin": "2.7.5",
    "node-sass": "^8.0.0",
    "plotly.js": "^2.24.2",
    "precompiled-mqtt": "^4.3.14-beta",
    "prism-color-variables": "1.0.1",
    "prismjs": "1.29.0",
    "react": "17.0.2",
    "react-datepicker": "4.10.0",
    "react-diff-view": "3.0.3",
    "react-dom": "17.0.2",
    "react-json-view": "1.21.3",
    "react-router-dom": "6.9.0",
    "react-table": "7.8.0",
    "react-toastify": "9.1.2",
    "react-tooltip": "5.10.1",
    "refractor": "4.8.1",
    "sass": "1.59.3",
    "sass-loader": "^13.2.1",
    "stream": "0.0.2",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "uuid": "9.0.0",
    "vanilla-jsoneditor": "^0.17.7",
    "webpack": "5.76.3",
    "webpack-cli": "5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run cypress-functional"
    }
  }
}
